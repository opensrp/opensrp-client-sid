<form autocomplete="off" class="or clearfix" dir="ltr" id="edit_bayi"
    novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSL stylesheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 dir="auto" id="form-title">Edit Bayi</h3>
    <select data-default-lang="Bahasa" id="form-languages">
        <option data-dir="ltr" value="Bahasa">Bahasa</option>
        <option data-dir="ltr" value="en">English</option>
    </select>
    <label class="question note non-select ">
        <span class="question-label active"
            data-itext-id="/edit_bayi/generated_note_name_13:label" lang="Bahasa">Desa
            :
            <span class="or-output" data-value=" /edit_bayi/Village "></span>
        </span>
        <span class="question-label "
            data-itext-id="/edit_bayi/generated_note_name_13:label" lang="en">Village :
            <span class="or-output" data-value=" /edit_bayi/Village "></span>
        </span>
        <input name="/edit_bayi/generated_note_name_13" data-type-xml="string"
            readonly="readonly" type="text" />
    </label>
    <label class="question note non-select ">
        <span class="question-label active"
            data-itext-id="/edit_bayi/generated_note_name_14:label" lang="Bahasa">
            Dusun :
            <span class="or-output" data-value=" /edit_bayi/Sub-village "></span>
        </span>
        <span class="question-label "
            data-itext-id="/edit_bayi/generated_note_name_14:label" lang="en">Sub Village :
            <span class="or-output" data-value=" /edit_bayi/Sub-village "></span>
        </span>
        <input name="/edit_bayi/generated_note_name_14" data-type-xml="string"
            readonly="readonly" type="text" />
    </label>
    <label class="question non-select ">
        <span class="question-label active"
            data-itext-id="/edit_bayi/nama_bayi:label"
            lang="Bahasa">Nama
            Bayi
        </span>
        <span class="question-label "
            data-itext-id="/edit_bayi/nama_bayi:label"
            lang="en">
            Baby's Name
        </span>
        <span class="required">*</span>
        <span class="or-hint active"
            data-itext-id="/edit_bayi/nama_bayi:hint"
            lang="Bahasa">Nama
            Bayi yang diberikan Imunisasi
        </span>
        <span class="or-hint "
            data-itext-id="/edit_bayi/nama_bayi:hint"
            lang="en">Name
            of Baby Receiving Immunization
        </span>
        <input name="/edit_bayi/nama_bayi"
            data-required="true()" data-type-xml="string" type="text" />
        <span class="or-required-msg active" data-i18n="constraint.required" lang="">This
            field is required
        </span>
    </label>
    <fieldset class="question simple-select ">
        <fieldset>
            <legend>
                <span class="question-label active"
                    data-itext-id="/edit_bayi/jenis_kelamin:label"
                    lang="Bahasa">
                    Jenis Kelamin
                </span>
                <span class="question-label "
                    data-itext-id="/edit_bayi/jenis_kelamin:label"
                    lang="en">
                    Gender
                </span>
                <span class="required">*</span>
                <span class="or-hint active"
                    data-itext-id="/edit_bayi/jenis_kelamin:hint"
                    lang="Bahasa">
                    Jenis Kelamin Bayi/Balita Yang diberikan Imunisasi
                </span>
                <span class="or-hint "
                    data-itext-id="/edit_bayi/jenis_kelamin:hint"
                    lang="en">
                    Gender of Baby Receiving Immunization
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/edit_bayi/jenis_kelamin"
                        data-name="/edit_bayi/jenis_kelamin"
                        data-required="true()" data-type-xml="select1" type="radio"
                        value="male" />
                    <span class="option-label active"
                        data-itext-id="/edit_bayi/jenis_kelamin/male:label"
                        lang="Bahasa">
                        Laki-Laki (L)
                    </span>
                    <span class="option-label "
                        data-itext-id="/edit_bayi/jenis_kelamin/male:label"
                        lang="en">
                        Male (M)
                    </span>
                </label>
                <label class="">
                    <input name="/edit_bayi/jenis_kelamin"
                        data-name="/edit_bayi/jenis_kelamin"
                        data-required="true()" data-type-xml="select1" type="radio"
                        value="female" />
                    <span class="option-label active"
                        data-itext-id="/edit_bayi/jenis_kelamin/female:label"
                        lang="Bahasa">
                        Perempuan ( P)
                    </span>
                    <span class="option-label "
                        data-itext-id="/edit_bayi/jenis_kelamin/female:label"
                        lang="en">
                        Female (F)
                    </span>
                </label>
            </div>
        </fieldset>
        <span class="or-required-msg active" data-i18n="constraint.required" lang="">This
            field is required
        </span>
    </fieldset>
    <label class="question non-select ">
        <span class="question-label active"
            data-itext-id="/edit_bayi/tanggal_lahir:label"
            lang="Bahasa">
            Tanggal lahir
        </span>
        <span class="question-label "
            data-itext-id="/edit_bayi/tanggal_lahir:label"
            lang="en">
            DOB
        </span>
        <span class="required">*</span>
        <span class="or-hint active"
            data-itext-id="/edit_bayi/tanggal_lahir:hint"
            lang="Bahasa">
            Tanggal lahir Bayi
        </span>
        <span class="or-hint "
            data-itext-id="/edit_bayi/tanggal_lahir:hint"
            lang="en">
            Baby's date of birth
        </span>
        <input name="/edit_bayi/tanggal_lahir"
            data-constraint=".&lt; /edit_bayi/today " data-required="true()"
            data-type-xml="date" type="date" />
        <span class="or-constraint-msg active" data-i18n="constraint.invalid" lang="">Value
            not allowed
        </span>
        <span class="or-required-msg active" data-i18n="constraint.required" lang="">This
            field is required
        </span>
    </label>
    <label class="question non-select ">
        <span class="question-label active"
            data-itext-id="/edit_bayi/berat_badan_saat_lahir:label"
            lang="Bahasa">
            Berat badan saat Lahir
        </span>
        <span class="question-label "
            data-itext-id="/edit_bayi/berat_badan_saat_lahir:label"
            lang="en">
            Baby's birth weight
        </span>
        <span class="or-hint active"
            data-itext-id="/edit_bayi/berat_badan_saat_lahir:hint"
            lang="Bahasa">
            Berat badan lahir bayi
        </span>
        <span class="or-hint "
            data-itext-id="/edit_bayi/berat_badan_saat_lahir:hint"
            lang="en">
            Baby's birth weight
        </span>
        <input name="/edit_bayi/berat_badan_saat_lahir"
            data-type-xml="int" type="number" />
    </label>
    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/edit_bayi/start" data-preload="timestamp"
                data-preload-params="start" data-type-xml="dateTime" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/today" data-preload="date"
                data-preload-params="today" data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/deviceid" data-preload="property"
                data-preload-params="deviceid" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/simserial" data-preload="property"
                data-preload-params="simserial" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/phonenumber" data-preload="property"
                data-preload-params="phonenumber" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/end" data-preload="timestamp"
                data-preload-params="end" data-type-xml="dateTime" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/edit_bayi/existing_location"
                data-calculate=" /edit_bayi/Village " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/provinsi"
                data-calculate=" /edit_bayi/Province " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/kabupaten"
                data-calculate=" /edit_bayi/District " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/desa"
                data-calculate=" /edit_bayi/Village " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/dusun"
                data-calculate=" /edit_bayi/Sub-village " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/kecamatan"
                data-calculate=" /edit_bayi/Sub-district " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/usiakehamilan"
                data-calculate="int((( /edit_bayi/today  -  /edit_bayi/hpht ) div 7)+1)"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/desa_anak"
                data-calculate=" /edit_bayi/Village " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/child_existing_location"
                data-calculate=" /edit_bayi/Village " data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/meta/instanceID"
                data-calculate="concat('uuid:', uuid())" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/edit_bayi/formhub/uuid"
                data-calculate="'079f238907bb4ba1bda37fae404b0980'" data-type-xml="string"
                type="hidden" />
        </label>
    </fieldset>
</form>