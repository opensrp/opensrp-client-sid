          "label": {
            "Bahasa": "Berat lahir",
            "English": "Baby's weight (gram)"
          },
          "instance": {
            "openmrs_entity_id": "5916AAAAAAAAAAAAAAAAAAAAAAAAAAAA",
            "openmrs_entity": "concept"
          },
          "type": "integer"
        },
