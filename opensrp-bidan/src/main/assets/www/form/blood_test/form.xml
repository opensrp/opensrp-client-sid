<form autocomplete="off" class="or clearfix" dir="ltr" id="blood_test" novalidate="novalidate">\n
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSL stylesheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 dir="auto" id="form-title">Blood Test</h3>
    <select data-default-lang="Bahasa" id="form-languages">
        <option data-dir="ltr" value="Bahasa">Bahasa</option>
        <option data-dir="ltr" value="en">English</option>
    </select>
    <label class="question note non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/generated_note_name_13:label"
            lang="Bahasa">Desa :
            <span class="or-output" data-value=" /Blood_Test/Village "></span>
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/generated_note_name_13:label"
            lang="en">-
        </span>
        <input name="/Blood_Test/generated_note_name_13" data-type-xml="string" readonly="readonly"
            type="text" />
    </label>
    <label class="question note non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/generated_note_name_14:label"
            lang="Bahasa">Dusun :
            <span class="or-output" data-value=" /Blood_Test/Sub-village "></span>
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/generated_note_name_14:label"
            lang="en">-
        </span>
        <input name="/Blood_Test/generated_note_name_14" data-type-xml="string" readonly="readonly"
            type="text" />
    </label>
    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/td_sistolik:label"
            lang="Bahasa">TD (Sistolik)
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/td_sistolik:label" lang="en">Blood
            pressure (sistolic)
        </span>
        <span class="or-hint active" data-itext-id="/Blood_Test/td_sistolik:hint" lang="Bahasa">
            Menunjukkan tekanan darah (Sistolik) Ibu pada saat kunjungan ANC (mmHg)
        </span>
        <span class="or-hint " data-itext-id="/Blood_Test/td_sistolik:hint" lang="en">Indicating
            mother's blood pressure (systolic) on ANC check-up
        </span>
        <input name="/Blood_Test/td_sistolik" data-constraint=".&gt;0 and .&lt;1000"
            data-type-xml="int" type="number" />
        <span class="or-constraint-msg active"
            data-itext-id="/Blood_Test/td_sistolik:jr:constraintMsg" lang="Bahasa">Nilai yang
            dimasukkan tidak
            memenuhi syarat, mohon periksa kembali
        </span>
        <span class="or-constraint-msg " data-itext-id="/Blood_Test/td_sistolik:jr:constraintMsg"
            lang="en">The entered value doesn't meet
            the criteria, please check it again
        </span>
    </label>
    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/td_diastolik:label"
            lang="Bahasa">TD (Diastolik)
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/td_diastolik:label" lang="en">Blood
            pressure (Diastolic)
        </span>
        <span class="or-hint active" data-itext-id="/Blood_Test/td_diastolik:hint" lang="Bahasa">
            Menunjukkan tekanan darah (Diastolik) Ibu pada saat kunjungan ANC (mmHg)
        </span>
        <span class="or-hint " data-itext-id="/Blood_Test/td_diastolik:hint" lang="en">Indicating
            mother's blood pressure (dyastolic) on ANC check-up
        </span>
        <input name="/Blood_Test/td_diastolik" data-constraint=".&gt;0 and .&lt;1000"
            data-type-xml="int" type="number" />
        <span class="or-constraint-msg active"
            data-itext-id="/Blood_Test/td_diastolik:jr:constraintMsg" lang="Bahasa">Nilai yang
            dimasukkan tidak
            memenuhi syarat, mohon periksa kembali
        </span>
        <span class="or-constraint-msg " data-itext-id="/Blood_Test/td_diastolik:jr:constraintMsg"
            lang="en">The entered value doesn't meet
            the criteria, please check it again
        </span>
    </label>
    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/pulse:label" lang="Bahasa">
            Pulse
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/pulse:label" lang="en">Pulse</span>
        <input name="/Blood_Test/pulse" data-type-xml="int" type="number" />
    </label>
    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/Blood_Test/ahr:label" lang="Bahasa">
            Average Heart Rate
        </span>
        <span class="question-label " data-itext-id="/Blood_Test/ahr:label" lang="en">Average Heart
            Rate
        </span>
        <input name="/Blood_Test/ahr" data-type-xml="int" type="number" />
    </label>
    \n
    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/Blood_Test/start" data-preload="timestamp" data-preload-params="start"
                data-type-xml="dateTime" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/today" data-preload="date" data-preload-params="today"
                data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/deviceid" data-preload="property"
                data-preload-params="deviceid" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/simserial" data-preload="property"
                data-preload-params="simserial" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/phonenumber" data-preload="property"
                data-preload-params="phonenumber" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/end" data-preload="timestamp" data-preload-params="end"
                data-type-xml="dateTime" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/Blood_Test/existing_location" data-calculate=" /Blood_Test/Village "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/provinsi" data-calculate=" /Blood_Test/Province "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/kabupaten" data-calculate=" /Blood_Test/District "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/desa" data-calculate=" /Blood_Test/Village "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/dusun" data-calculate=" /Blood_Test/Sub-village "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/kecamatan" data-calculate=" /Blood_Test/Sub-district "
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/meta/instanceID" data-calculate="concat('uuid:', uuid())"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Blood_Test/formhub/uuid"
                data-calculate="'ed9829dab8ac453997cde171feeac09e'" data-type-xml="string"
                type="hidden" />
        </label>
    </fieldset>
</form>